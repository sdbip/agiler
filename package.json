  "scripts": {
    "deyaml": "yaml2json tsconfig.yml --save",
    "bundle-client": "yarn webpack && rm -rf lib",
    "start": "yarn bundle-client && source .env && ts-node-esm run.ts",
    "lint": "eslint frontend src test",
    "test": "source .env && mocha -R dot && wtr --node-resolve --esbuild-target auto",
    "build": "yarn deyaml && yarn test && yarn lint && echo 'Build OK'"
  },
  "devDependencies": {
    "@esm-bundle/chai": "^4.3.4-fix.0",
    "@types/chai": "^4.3.3",
    "@types/express": "^4.17.14",
    "@types/mocha": "^9.1.1",
    "@types/mustache": "^4.2.1",
    "@types/node": "^18.7.18",
    "@types/pg": "^8.6.5",
    "@typescript-eslint/eslint-plugin": "^5.37.0",
    "@typescript-eslint/parser": "^5.37.0",
    "@web/dev-server-esbuild": "^0.3.2",
    "@web/test-runner": "^0.14.0",
    "chai": "^4.3.6",
    "eslint": "^8.23.1",
    "mocha": "^10.0.0",
    "mustache": "^4.2.0",
    "ts-loader": "^9.4.1",
    "ts-node": "^10.9.1",
    "tslib": "^2.3.0",
    "typescript": "^4.4.3",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0",
    "yamljs": "^0.3.0"
  },
  "dependencies": {
    "express": "^4.18.1",
    "pg": "^8.8.0"
  }
